package org.kh.java;

public class Variable7 {

	public static void main(String[] args) {
		String name = "김동연";
		Integer age = 33;
		Float height = 170.6f;
		boolean pass = true;
		double weight = 73.4;
		
		System.out.println("name : "+name.getClass().getName());
		System.out.println("age : "+age.getClass().getName());
		System.out.println("height : "+height.getClass().getName());

	}

}
