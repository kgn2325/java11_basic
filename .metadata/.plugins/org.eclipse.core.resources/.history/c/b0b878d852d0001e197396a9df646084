package org.kh.site2;
// 대입 연산자 : 연산 후에 대입하는 연산자 : +=, -=, *=, /=, %=, &=, |=, >>=, <<=, ^=
public class Operation7 {

	public static void main(String[] args) {
		int a = 20;
		int b = 15;
		int c;
		a += b; // a=35, b=15
		b -= a; // a=35, b=-20
		System.out.println("a = "+a+", b = "+b);
		c=a;
		a=b;
		b=c;
		System.out.println("a = "+a+", b = "+b);
		
		

	}

}
